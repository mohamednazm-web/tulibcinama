extends base

block content
    .profiles__title(href="#")
      p پرۆفایلەکان
    section.profiles
        .profiles__title(href="#")
          p
        .pros
            each p in developer
                if p.description
                  a.pro(href=`/member/${p.slugMember}`)
                    if p.imageProfile
                      img.pro__img(src=`/images/profile/${p.imageProfile}`, alt="" title="Join to his Facebook" loading="lazy")
                    else if p.imageProfileUpload
                           img.pro__img(src=`/${p.imageProfileUpload}`, alt="" title="Join to his Facebook" loading="lazy")
                    .realtors__details
                      p.realkljtors__sold= p.memberName
                      h4.heading-4.heading-4--light= p.typeAction
                else
                  a.pro(href=`${p.href}`)
                    if p.imageProfile
                      img.pro__img(src=`/images/profile/${p.imageProfile}`, alt="" title="Join to his Facebook" loading="lazy")
                    else if p.imageProfileUpload
                           img.pro__img(src=`/${p.imageProfileUpload}`, alt="" title="Join to his Facebook" loading="lazy")
                    .realtors__details
                      p.realkljtors__sold= p.memberName
                      h4.heading-4.heading-4--light= p.typeAction
    section.profiles
        .profiles__title(href="#")
          p نەخشاندنی پۆستەرەکان لەلایەن
        .pros
            each p in naxshandn
                a.pro()
                  if p.imageProfile
                    img.pro__img(src=`/images/profile/${p.imageProfile}`, alt="")
                  else if p.imageProfileUpload
                         img.pro__img(src=`/${p.imageProfileUpload}`, alt="")
                  .realtors__details
                    p.realkljtors__sold= p.memberName
    section.profiles
        .profiles__title(href="#")
          p وەرگێڕ و نووسەرەکان
        .pros
            each p in profile
                a.pro(href=`/member/${p.slugMember}`)
                  if p.imageProfile
                    img.pro__img(src=`/images/profile/${p.imageProfile}`, alt="")
                  else if p.imageProfileUpload
                         img.pro__img(src=`/${p.imageProfileUpload}`, alt="")
                  .realtors__details
                    p.realkljtors__sold= p.memberName
                    h4.heading-4.heading-4--light= `(${p.numOfPosters}) ژمارەی بەرهەم`