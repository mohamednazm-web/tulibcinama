extends base

block content
    section.posterOne
    .posterOne__title(href="#")
        p title
    .posters

        each m in member
            .poster
              a.poster__link(href=`/movies/${m.slug}`)
                img.poster__img(src=`/img/posters/${m.photo}`, alt="Poster 1")
                p.poster__titleFilm= m.title